
<div class="login">
<%= link_to "Sign Up", new_user_path %>
<%= link_to "Log In", user_sessions_path %>



<%= form_for(:subject, url: subjects_path) do |f| %>
<%= f.text_field(:subject) %>
<%= submit_tag("Create Subject") %>
<% end %>
</div>


<div class="subjectlist">
<% @subjects.each do |subject|  %>
  <%= link_to(subject.subject, subject) %><br/>
  <% end %>
</div>












